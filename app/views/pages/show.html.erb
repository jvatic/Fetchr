<p id="notice"><%= notice %></p>

<h1><%= @page.title %></h1>

<h2>Links</h2>

<ul>
  <% @page.links.each do |link| %>
    <li><a href='<%= link.url %>' <%= resource_attributes(link) %>><%= link.url %></a>
  <% end %>
</ul>

<h2>Images</h2>

<ul>
  <% @page.images.each do |image| %>
    <li><img src='<%= image.url %>' <%= resource_attributes(image) %> /></li>
  <% end %>
</ul>
